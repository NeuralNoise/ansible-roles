#!/bin/sh


for APP in {{ apps }};
do
    . /var/venvs/influx-trending/bin/activate && python /var/influx-trending/percentile.py "{{ host }}" "{{ port }}" "{{ username }}" "{{ password }}" "{{ db }}" "$APP.rollup.1h.percentile.99" "$APP.rollup.1h" "$APP.rollup.1h.popular.99" "{{ hourly_offset }}" && deactivate
done
exit 0
