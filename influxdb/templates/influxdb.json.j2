{
  "spaces": [
    {
      "name": "default",
      "retentionPolicy": "7d",
      "shardDuration": "1d",
      "regex": "/.*/",
      "replicationFactor": {{ influxdb_repl_factor }},
      "split": 1
    },
    {
      "name": "5m_rollups",
      "retentionPolicy": "30d",
      "shardDuration": "7d",
      "regex": "/.*\\.5m/",
      "replicationFactor": {{ influxdb_repl_factor }},
      "split": 1
    },
    {
      "name": "1h_rollups",
      "retentionPolicy": "60d",
      "shardDuration": "7d",
      "regex": "/.*\\.1h/",
      "replicationFactor": {{ influxdb_repl_factor }},
      "split": 1
    },
    {
      "name": "1d_rollups",
      "retentionPolicy": "365d",
      "shardDuration": "30d",
      "regex": "/.*\\.1d/",
      "replicationFactor": {{ influxdb_repl_factor }},
      "split": 1
    }
  ]
}
